{
  "$schema" : "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name" : "Redis connector",
  "id" : "io.camunda.connector.Template.v1",
  "description" : "Redis connector allows to interact with Redis cache. It supports get (read), put (write) and delete (remove cache record) operations. Supported data format is <key,value> pair, where key is a valid String and value is JSON.",
  "documentationRef" : "https://docs.camunda.io/docs/components/connectors/out-of-the-box-connectors/available-connectors-overview/",
  "version" : 1,
  "category" : {
    "id" : "connectors",
    "name" : "Connectors"
  },
  "appliesTo" : [ "bpmn:ServiceTask" ],
  "groups" : [ {
    "id" : "authentication",
    "label" : "Authentication"
  }, {
    "id" : "compose",
    "label" : "Compose"
  }, {
    "id" : "output",
    "label" : "Output mapping"
  }, {
    "id" : "error",
    "label" : "Error handling"
  }, {
    "id" : "retries",
    "label" : "Retries"
  } ],
  "properties" : [ {
    "value" : "io.camunda:redis-connector-nodejs:1",
    "binding" : {
      "type" : "zeebe:taskDefinition:type"
    },
    "type" : "Hidden"
  }, {
    "id" : "hostname",
    "label" : "Hostname",
    "description" : "Host server name, preferably FQDN",
    "optional" : false,
    "constraints" : {
      "notEmpty" : true
    },
    "feel" : "optional",
    "group" : "compose",
    "binding" : {
      "name" : "hostname",
      "type" : "zeebe:input"
    },
    "type" : "String",
    "value": "hostname-fqdn"
  }, {
    "id" : "port",
    "label" : "Port",
    "description" : "Port number",
    "optional" : false,
    "constraints" : {
      "notEmpty" : true,
      "minLength" : 2,
      "pattern" : {
        "value" : "^[1-9][0-9]*"
      }
    },
    "feel" : "optional",
    "group" : "compose",
    "binding" : {
      "name" : "port",
      "type" : "zeebe:input"
    },
    "type" : "String",
    "value": "11485"
  }, {
    "id" : "user",
    "label" : "Username",
    "description" : "The username for authentication",
    "optional" : false,
    "constraints" : {
      "notEmpty" : true
    },
    "feel" : "optional",
    "group" : "compose",
    "binding" : {
      "name" : "user",
      "type" : "zeebe:input"
    },
    "type" : "String",
    "value": "uname"
  }, {
    "id" : "token",
    "label" : "Token",
    "description" : "The token or password for authentication",
    "optional" : false,
    "constraints" : {
      "notEmpty" : true
    },
    "feel" : "optional",
    "group" : "compose",
    "binding" : {
      "name" : "token",
      "type" : "zeebe:input"
    },
    "type" : "String",
    "value": "=\"secrets.REDIS_SECRET\""
  }, {
    "id" : "operationType",
    "label" : "Operation type",
    "optional" : false,
    "constraints" : {
      "notEmpty" : true
    },
    "group" : "compose",
    "binding" : {
      "name" : "operationType",
      "type" : "zeebe:input"
    },
    "type" : "Dropdown",
    "choices" : [ {
      "name" : "DELETE",
      "value" : "DELETE"
    }, {
      "name" : "GET",
      "value" : "GET"
    }, {
      "name" : "PUT",
      "value" : "PUT"
    } ], 
    "value" : "GET"
  }, {
    "id" : "key",
    "label" : "Key",
    "optional" : false,
    "constraints" : {
      "notEmpty" : true
    },
    "feel" : "optional",
    "group" : "compose",
    "binding" : {
      "name" : "key",
      "type" : "zeebe:input"
    },
    "type" : "Text",
    "value": "makelabs"
  }, {
    "id" : "resultVariable",
    "label" : "Result variable",
    "description" : "Name of variable to store the response in",
    "group" : "output",
    "binding" : {
      "key" : "resultVariable",
      "type" : "zeebe:taskHeader"
    },
    "type" : "String",
    "value": "jsonData"
  }, {
    "id" : "resultExpression",
    "label" : "Result expression",
    "description" : "Expression to map the response into process variables",
    "feel" : "required",
    "group" : "output",
    "binding" : {
      "key" : "resultExpression",
      "type" : "zeebe:taskHeader"
    },
    "type" : "Text"
  }, {
    "id" : "errorExpression",
    "label" : "Error expression",
    "description" : "Expression to handle errors. Details in the <a href=\"https://docs.camunda.io/docs/components/connectors/use-connectors/\" target=\"_blank\">documentation</a>.",
    "feel" : "required",
    "group" : "error",
    "binding" : {
      "key" : "errorExpression",
      "type" : "zeebe:taskHeader"
    },
    "type" : "Text"
  }, {
    "id" : "retryBackoff",
    "label" : "Retry backoff",
    "description" : "ISO-8601 duration to wait between retries",
    "value" : "PT0S",
    "group" : "retries",
    "binding" : {
      "key" : "retryBackoff",
      "type" : "zeebe:taskHeader"
    },
    "type" : "Hidden"
  } ],
  "icon" : {
    "contents" : "data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIGhlaWdodD0iMTgiIHdpZHRoPSIxOCIgdmlld0JveD0iMCAwIDEwIDEwIiBzaGFwZS1yZW5kZXJpbmc9Imdlb21ldHJpY1ByZWNpc2lvbiI+PHRpdGxlPlNsYWNrPC90aXRsZT48ZyBmaWxsPSJub25lIj48cGF0aCBkPSJNMCwwIEwwLDEwIEwxMCwxMCBMMTAsMCB6IiBmaWxsPSIjZWNiMTJmIi8+PC9nPjwvc3ZnPg=="
  }
}